{
  "name": "@mindfiredigital/page-builder-react",
  "version": "1.1.3",
  "private": false,
  "description": "React UI Page builder",
  "repository": {
    "type": "git",
    "url": "https://github.com/mindfiredigital/page-builder.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "start": "serve dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "release": "turbo run build && pnpm publish --access public"
  },
  "dependencies": {
    "@mindfiredigital/page-builder-core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.0.0",
    "tsup": "^6.7.0",
    "serve": "^14.2.4",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
}
