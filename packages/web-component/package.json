{
  "name": "@mindfiredigital/page-builder-web-component",
  "description": "UI Page builder Web-Component",
  "private": false,
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mindfiredigital/page-builder.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "start": "serve dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "keywords": [
    "pagebuilder",
    "page-builder-web-component",
    "rich-page-builder",
    "html-editor",
    "typescript",
    "npm-package",
    "block-editor",
    "visual-editor",
    "no-code",
    "low-code",
    "component-based",
    "responsive-design"
  ],
  "peerDependencies": {
    "@mindfiredigital/page-builder": "workspace:*"
  },
  "devDependencies": {
    "eslint": "^9.19.0",
    "tsup": "^6.7.0",
    "typescript": "^5.7.3",
    "serve": "^14.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
